[user]
	name = Martin Colwell
	email = colwem@gmail.com
[core]
	excludesfile = C:\\Users\\ColwellBC\\.config\\git\\ignore
	autocrlf = input
	editor = mvim
[push]
	default = simple
[branch]
	autosetupmerge = true
[apply]
	whitespace = nowarn
[color]
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[alias]
	st = status
	ci = commit
	br = branch
	co = checkout
	df = diff
	dc = diff --cached
	lg = log -p
	lol = log --graph -decorate --pretty=oneline --abbrev-commit
	lola = log --graph -decorate --pretty=oneline --abbrev-commit --all
	ls = ls-files
	ign = ls-files -o -i --exclude-standard
[url "https://"]
	insteadOf = git://
